/var/vcap/store/loghost/*/*/*.log {
 rotate <%= p('loghost_concentrator.logrotate.default-max-hours') %>
 hourly
 missingok
 compress
 copytruncate
 dateext
 su vcap vcap
}

/var/vcap/store/loghost_lt/*/*/*.log {
 rotate <%= p('loghost_concentrator.logrotate.longterm-max-hours') %>
 hourly
 missingok
 compress
 copytruncate
 dateext
 su vcap vcap
}
